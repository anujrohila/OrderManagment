@using OrderManagement.Web.UI
@using OrderManagement.Web.Domain.Resources
@using Telerik.Web.Mvc.UI;

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    @RenderSection("Head", false)
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='icon' href="@Url.Content("~/Contents/Images/Shared/favicon.ico")" type='image/x-icon'>
    <link rel='shortcut icon' href="@Url.Content("~/Contents/Images/Shared/favicon.ico")" type='image/x-icon'>

    <style type="text/css">
        body {
            padding-bottom: 40px;
        }

        .sidebar-nav {
            padding: 9px 0;
        }
    </style>

    @Html.Style("~/Content/Styles/Shared/Common.css")
    @Html.Style("~/plugins/bootstrap/bootstrap.css")
    @Html.Style("~/css/style.css")
    <link href="http://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Righteous' rel='stylesheet' type='text/css'>
</head>
<body>
    <div class="container-fluid">
        <!-- content starts -->
        @RenderSection("featured", required: false)
        @RenderBody()
    </div>
    <script type="text/javascript" src="~/Content/Scripts/Shared/jquery-1.8.1.min.js"></script>
    <script type="text/javascript" src="~/Content/Scripts/Shared/jquery.validate.js"></script>
    <script type="text/javascript" src="~/Content/Scripts/Shared/jquery.validate.unobtrusive.js"></script>
    <script src="~/Base/OrderManagementResource" type="text/javascript"></script>
</body>
</html>

