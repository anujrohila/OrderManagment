@using OrderManagement.Web.Domain.Resources;
@model OrderManagement.Web.Domain.tblOrganizationDTO
@{
    ViewBag.Title = "Register Success";
    Layout = "~/Views/Shared/_AdminLoginLayout.cshtml";
}
<div id="page-login" class="row">
    <div class="col-xs-12 col-md-4 col-md-offset-4 col-sm-6 col-sm-offset-3">
        <div class="text-right">
        </div>
        <div class="box">
            <div class="box-content">
                <div class="text-center">
                    <h3 class="page-header">@OrderManagementResource.lblRegisterSuccess</h3>
                </div>
                <div class="form-group">
                    <div class="box-content">
                        <p style="color: #63cc9e; text-align: center; vertical-align: central">@OrderManagementResource.msgRegisterSuccess</p>
                    </div>
                </div>
                <div class="text-center">
                    <a style="width: 150px" class="btn btn-primary" href="~/Account/Login">@OrderManagementResource.lblLogin</a>
                </div>
            </div>
        </div>
    </div>
</div>

